{
	"name": "lobx",
	"version": "0.2.0-rc21",
	"description": "a lightweight proxy based alternative to mobx",
	"main": "lib/lobx.js",
	"module": "lib/lobx.module.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/*"
	],
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cover": "jest --coverage",
		"test:perf": "npm run build:prod && node  node_modules/.bin/jest --testRegex perf.ts",
		"build": "rollup -c rollup.config.js",
		"build:prod": "rollup -c rollup.config.js",
		"lint": "eslint ./src ./tests --ext .ts",
		"prepublishOnly": "npm run build:prod"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/melnikov-s/lobx.git"
	},
	"author": "Sergey Melnikov",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/melnikov-s/lobx/issues"
	},
	"homepage": "https://github.com/melnikov-s/lobx#readme",
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/preset-env": "^7.12.16",
		"@types/jest": "^26.0.20",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"babel-loader": "^8.2.2",
		"eslint": "^7.20.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^7.2.0",
		"eslint-import-resolver-typescript": "^2.4.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.3.1",
		"jest": "^26.6.3",
		"mobx": "^6.1.6",
		"prettier": "^2.2.1",
		"rollup": "^2.39.0",
		"rollup-plugin-typescript2": "^0.29.0",
		"ts-jest": "^26.5.1",
		"tslib": "^2.1.0",
		"typescript": "^4.1.5"
	}
}
